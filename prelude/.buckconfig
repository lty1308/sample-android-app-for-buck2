[repositories]
prelude = .
buck = buck_client
shim = shim

[repository_aliases]
config = prelude
ovr_config = prelude
fbcode = shim
fbcode_macros = shim
fbsource = shim

[parser]
target_platform_detector_spec = target:root//...->prelude//platforms:default target:shim//...->prelude//platforms:default

[buildfile]
name = TARGETS

